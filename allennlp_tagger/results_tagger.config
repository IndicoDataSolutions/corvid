{
  "dataset_reader": {
    "type": "sequence_tagging"
  },
  "train_data_path": "../data/papers-2017-10-30.induced_labels.train",
  "validation_data_path": "../data/papers-2017-10-30.induced_labels.dev",
  "model": {
    "type": "simple_tagger",
    "text_field_embedder": {
      "tokens": {
        "type": "embedding",
      	"pretrained_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/glove/glove.6B.100d.txt.gz",
	"embedding_dim": 100,
	"trainable": false
      }
    },
    "stacked_encoder": {
      "type": "gru",
      "input_size": 100,
      "hidden_size": 300,
      "num_layers": 2,
      "dropout": 0.5,
      "bidirectional": true
    }
  },
  "iterator": {
    "type": "basic",
    "batch_size": 32
  }, 
  "trainer": {
    "num_epochs": 2,
    "patience": 10,
    "grad_clipping": 5.0,
    "optimizer": {
      "type": "adagrad"
    }
  }
}
